{
  "type": "object",
  "properties": {
    "ovUrl": {
      "type": "string",
      "pattern": "^https://.*"
    },
    "ovAccessKey": {
      "type": "string",
      "minLength": 20,
      "maxLength": 20
    },
    "ovSecretKey": {
      "type": "string",
      "minLength": 70,
      "maxLength": 70
    },
    "qrcodeFillColor": {
      "type": "string",
      "minLength": 3,
      "maxLength": 30
    },
    "qrcodeBackgroudColor": {
      "type": "string",
      "minLength": 3,
      "maxLength": 30
    }
  },
  "required": [
    "ovUrl",
    "ovAccessKey",
    "ovSecretKey",
    "qrcodeFillColor",
    "qrcodeBackgroudColor"
  ]
}
