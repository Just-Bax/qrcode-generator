{
  "type": "object",
  "properties": {
    "ovUrl": {
      "type": "string",
      "pattern": "^https://.*"
    },
    "ovAccessKey": {
      "type": "string",
      "minLength": 20,
      "maxLength": 20
    },
    "ovSecretKey": {
      "type": "string",
      "minLength": 70,
      "maxLength": 70
    },
    "qrcodeFillColor": {
      "type": "string"
    },
    "qrcodeBackgroudColor": {
      "type": "string"
    }
  },
  "required": [
    "ovUrl",
    "ovAccessKey",
    "ovSecretKey",
    "qrcodeFillColor",
    "qrcodeBackgroudColor",
    "trackorType",
    "qrcodeGenerateField",
    "qrcodeImageField",
    "qrcodeTitleField",
    "qrcodeValueField"
  ]
}
